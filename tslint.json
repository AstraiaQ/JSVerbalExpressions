{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
      "quotemark": [true, "double", "avoid-template"],
      "trailing-comma": {
        "multiline": "never",
        "singleline": "never"
      },
      "max-line-length": false,
      "no-empty": false,
      "interface-name": false
    },
    "rulesDirectory": []
}
